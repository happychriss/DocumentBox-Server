<h1 style="color: green" id="notice"><%= flash[:notice] %></h1>
<h1 style="color: red" id="error"><%= flash[:error] %></h1>
<h1>Uploads a file</h1>
<div class="input_area">
  <%= form_for :file_upload, :url => {:action => 'create_from_upload'}, :html => {:multipart => true} do |f| %>
      <%= f.file_field :my_file %></br></br>
      <%= f.submit "Upload" %>
  <% end %>
</div>