<div id="keywords">
  <div class="form-field">
    <% Tag.all.each do |tag| %>
        <div style="float:left;min-width:70px">
          <%= check_box_tag("document[keyword_list][]", tag.name, current_keywords.include?(tag.id) || current_keywords.include?(tag.name)) %>
          <label><%= tag.name %>  </label>
        </div>
    <% end %>

    <div class="clear"></div>
  </div>
</div>


